<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form">
	<f:SimpleForm  id="idInfoForm">
		<f:content>
			<Label text="Product Name">
			</Label>
			<Text text="{NAME}">
			</Text>
			<Label text="Description">
			</Label>
			<Text text="{DESCRIPTION}">
			</Text>
			<Label text="Price">
			</Label>
			<Text text="{PRICE} {CURRENCY_CODE}">
			</Text>
			<Label text="Supplier">
			</Label>
			<Text text="{SUPPLIER_NAME} - {SUPPLIER_ID}"></Text>
			<core:Title text ="Supplier Data"></core:Title>	
	    	<Label text="Email_Address">
			</Label>
			<Text text="{To_Supplier/EMAIL_ADDRESS}">
			</Text>
			<Label text="Phone Number ">
			</Label>
			<Text text="{To_Supplier/PHONE_NUMBER}">
			</Text>
			<Label text="Address">
			</Label>
			<Text text="{To_Supplier/CITY},{To_Supplier/COUNTRY}">
			</Text>
		</f:content>
        <!-- <Button text="Submit" press="onSubmit"></Button> -->
	</f:SimpleForm>
</core:FragmentDefinition>
